    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@tanstack/react-query": "^5.55.4",
    "autoprefixer": "10.4.17",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "geist": "^1.2.1",
    "lucide-react": "^0.436.0",
    "next": "^14.2.13",
    "next-themes": "^0.3.0",
    "openai": "^4.61.0",
