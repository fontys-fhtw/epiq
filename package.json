{
  "name": "epiq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "test": "jest",
    "test:all": "npm run test -- --watchAll=false --bail",
    "test:report": "npm run test -- --testResultsProcessor=jest-junit --coverage",
    "test:watch": "npm run test -- --watch --onlyChanged",
    "lint": "next lint",
    "lint:staged": "npm run lint -- --quiet --fix",
    "prettier:staged": "prettier --write --ignore-unknown ./"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:staged",
      "npm run prettier:staged"
    ],
    "*.{json,html}": [
      "npm run prettier:staged"
    ]
  },
  "dependencies": {
